<!doctype html>

<html lang="en">

{{> head}}

<body>

  {{> header}}

  {{#museum}}
  <img src="/images/museum.jpg" class="museum_photo" />

  <div class="museum">
    <p><b>Name</b>: {{name}}</p>
    <p><b>Country</b>: {{country}}</p>
    <p><b>Founded</b>: {{founded}}</p>
    <p><b>Artists presented</b>: {{artistNum}}</p>
    <p><b>Exhibitions</b>: {{exhibitNum}}</p>
  </div>

  {{/museum}}

  {{> footer}}
</body>

</html>