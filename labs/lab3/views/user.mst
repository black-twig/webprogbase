<!doctype html>

<html lang="en">

  {{> head}}

<body>

  <header id="header">
    <div class="name">Art Museum Association</div>

    <img src="/images/logo.png" class="logo" />

    <nav class="menu">
      <ul>
        <li><a href="/">Home</a></li>
        <li><a href="/users">Users</a></li>
        <li><a href="/museums">Museums</a></li>
        <li><a href="/about">About</a></li>
      </ul>
    </nav>
  </header>

  {{#user}}
  <img src="/images/user.jpg" class="usr_ava" />
  <div class="user">
    <p><h2>{{fullname}}</h2></p>
    <p>({{login}}) {{registeredAt}} </p>
    <div class="bio">
      <p> Just a usual user ;) </p>
    </div>
  </div>

  {{/user}}
</body>

</html>